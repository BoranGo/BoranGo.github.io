  edge	[ color=forestgreen	# represents positive effects (adding value)
	  penwidth=2		# width 2 is easier to click on
 	  fontsize=10		# smaller than the default of 12 in nodes

	  dir=both              # draw bidirectional edges by default...
          arrowtail=none        # ... but without an arrowtail, typically.
                                # This is useful for edges which have different shapes e.g. UML aggregation
  ]

