digraph 
{
  splines    = true
  remincross = true

  nodesep="0.2"
  ranksep="0.2"

  rankdir =BT	                # most 'valuable' (or value-enriched) things go on top;
                                # supporting things (infrastructure) go to the bottom
          
          tooltip = " "         # graph and node tooltips shall not use default ids (edge tooltips are always set by KTS)
  node  [ tooltip = " " ]

  node  [ fontname=Helvetica margin=0.1 width=0 height=0 ] # render nodes as compact as possible

  graph [ style="filled,rounded"     color=whitesmoke labelloc=b fontname=Helvetica ]

  graph [ target="details" ]    # open hyperlinks in a separate browser tab named 'details'
  node  [ target="details" ]
  edge  [ target="details" ]

  node  [ shape=box        style="filled,rounded" fillcolor=white ]

  edge	[ color=forestgreen	# represents positive effects (adding value)
	  penwidth=2		# width 2 is easier to click on
 	  fontsize=10		# smaller than the default of 12 in nodes

	  dir=both              # draw bidirectional edges by default...
          arrowtail=none        # ... but without an arrowtail, typically.
                                # This is useful for edges which have different shapes e.g. UML aggregation
  ]

  node  [ id="\N"      ] # essential for KTS JavaScript traversing SVG
  edge  [ id="\T___\H" ] # essential for KTS JavaScript traversing SVG
